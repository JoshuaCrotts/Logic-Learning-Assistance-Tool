\documentclass[tikz,border=10pt]{standalone}
\newcommand*\lif{\mathbin{\to}}% added thanks to egreg's suggestion
\usepackage{forest}
\usepackage{amsfonts}
\newcommand{\varlnot}{\mathord{\sim}}
\newcommand{\varland}{\mathbin{\&}}
\newcommand{\varliff}{\leftrightarrow}
\newcommand{\dneg}{\varlnot\varlnot}
\forestset{
  smullyan tableaux/.style={
    for tree={
        s sep'=20ex,
        math content,
        parent anchor=south,
        child anchor=north
    },
    where n children=1{
      !1.before computing xy={l=\baselineskip},
      !1.no edge,
    }{},
    closed/.style={
      label=below:$\times$
    },
    open/.style={
      label=below:open
    },
    uni/.style={
    label=right:*
    },
    uns/.style={
    label=right:?
    },
  },
}
\begin{document}
\begin{forest}
smullyan tableaux
[(((x)(Lax \lor Max) \varliff Lax) \mathbin{\&} (\varlnot Sax \to Lax))
	[((x)(Lax \lor Max) \varliff Lax)
		[(\varlnot Sax \to Lax)
			[\varlnot \varlnot Sax
				[Sax
					[(x)(Lax \lor Max), uni
						[Lax
							[(Laa \lor Maa)
								[Laa, open
								]
								[Maa, open
								]
							]
						]
					]
					[\varlnot (x)(Lax \lor Max)
						[\varlnot Lax
							[(\exists x)\varlnot (Lax \lor Max)
								[\varlnot (Lab \lor Mab)
									[\varlnot Lab
										[\varlnot Mab, open
										]
									]
								]
							]
						]
					]
				]
			]
			[Lax
				[(x)(Lax \lor Max), uni
					[Lax
						[(Laa \lor Maa)
							[Laa, open
							]
							[Maa, open
							]
						]
					]
				]
				[\varlnot (x)(Lax \lor Max)
					[\varlnot Lax, closed
					]
				]
			]
		]
	]
]
\end{forest}
\end{document}
