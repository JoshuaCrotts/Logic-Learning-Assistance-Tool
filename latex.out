\documentclass[tikz,border=10pt]{standalone}
\newcommand*\lif{\mathbin{\to}}% added thanks to egreg's suggestion
\usepackage{forest}
\usepackage{amsfonts}
\newcommand{\varlnot}{\mathord{\sim}}
\newcommand{\varland}{\mathbin{\&}}
\newcommand{\varliff}{\leftrightarrow}
\newcommand{\dneg}{\varlnot\varlnot}
\forestset{
  smullyan tableaux/.style={
    for tree={
        s sep'=20ex,
        math content,
        parent anchor=south,
        child anchor=north
    },
    where n children=1{
      !1.before computing xy={l=\baselineskip},
      !1.no edge,
    }{},
    closed/.style={
      label=below:$\times$
    },
    open/.style={
      label=below:open
    },
    uni/.style={
    label=right:*
    },
    uns/.style={
    label=right:?
    },
  },
}
\begin{document}
\begin{forest}
smullyan tableaux
[((A \to (\varlnot B \mathbin{\&} (\varlnot C \varliff \varlnot A))) \lor (\varlnot B \mathbin{\&} \varlnot C))
	[(A \to (\varlnot B \mathbin{\&} (\varlnot C \varliff \varlnot A)))
		[\varlnot A, open
		]
		[(\varlnot B \mathbin{\&} (\varlnot C \varliff \varlnot A))
			[\varlnot B
				[(\varlnot C \varliff \varlnot A)
					[\varlnot C
						[\varlnot A, open
						]
					]
					[\varlnot \varlnot C
						[\varlnot \varlnot A
							[C
								[A, open
								]
							]
						]
					]
				]
			]
		]
	]
	[(\varlnot B \mathbin{\&} \varlnot C)
		[\varlnot B
			[\varlnot C, open
			]
		]
	]
]
\end{forest}
\end{document}
