\documentclass[tikz,border=10pt]{standalone}
\newcommand*\lif{\mathbin{\to}}% added thanks to egreg's suggestion
\usepackage{forest}
\usepackage{amsfonts}
\newcommand{\varlnot}{\mathord{\sim}}
\newcommand{\varland}{\mathbin{\&}}
\newcommand{\varliff}{\leftrightarrow}
\newcommand{\dneg}{\varlnot\varlnot}
\forestset{
  smullyan tableaux/.style={
    for tree={
        s sep'=20ex,
        math content,
        parent anchor=south,
        child anchor=north
    },
    where n children=1{
      !1.before computing xy={l=\baselineskip},
      !1.no edge,
    }{},
    closed/.style={
      label=below:$\times$
    },
    open/.style={
      label=below:open
    },
    uni/.style={
    label=right:*
    },
    uns/.style={
    label=right:?
    },
  },
}
\begin{document}
\begin{forest}
smullyan tableaux
[\varlnot ((\varlnot P \mathbin{\&} (\varlnot Q \to R)) \varliff \varlnot (P \lor (\varlnot Q \mathbin{\&} \varlnot R)))
	[(\varlnot P \mathbin{\&} (\varlnot Q \to R))
		[\varlnot \varlnot (P \lor (\varlnot Q \mathbin{\&} \varlnot R))
			[(P \lor (\varlnot Q \mathbin{\&} \varlnot R))
				[\varlnot P
					[(\varlnot Q \to R)
						[P, closed
						]
						[(\varlnot Q \mathbin{\&} \varlnot R)
							[\varlnot Q
								[\varlnot R
									[\varlnot \varlnot Q
										[Q, closed
										]
									]
									[R, closed
									]
								]
							]
						]
					]
				]
			]
		]
	]
	[\varlnot (\varlnot P \mathbin{\&} (\varlnot Q \to R))
		[\varlnot (P \lor (\varlnot Q \mathbin{\&} \varlnot R))
			[\varlnot P
				[\varlnot (\varlnot Q \mathbin{\&} \varlnot R)
					[\varlnot \varlnot P
						[P, closed
						]
					]
					[\varlnot (\varlnot Q \to R)
						[\varlnot Q
							[\varlnot R
								[\varlnot \varlnot Q
									[Q, closed
									]
								]
								[\varlnot \varlnot R
									[R, closed
									]
								]
							]
						]
					]
				]
			]
		]
	]
]
\end{forest}
